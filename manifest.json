{

  "description": "Show and remove expired bookmarks",
  "manifest_version": 2,
  "name": "404 Bookmarks",
  "version": "1.8 beta",

  "icons": {
    "48": "icon.svg"
  },

  "permissions": [
    "bookmarks",
    "<all_urls>",
    "storage"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "404 Bookmarks",
    "default_icon": "icon.svg"
  },

  "options_ui": {
        "page": "options.html",
        "browser_style": true
    },

  "applications": {
      "gecko": {
          "id": "404-bookmarks@testing"
      }
  }


}
