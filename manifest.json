{

  "description": "Show and remove expired bookmarks",
  "manifest_version": 2,
  "name": "404 Bookmarks",
  "version": "1.7",

  "icons": {
    "48": "icon.svg"
  },

  "permissions": [
    "bookmarks", "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "404 Bookmarks",
    "default_icon": "icon.svg"
  }
}
